-- Scripts:

local function ZPGV_fake_script() -- Tab1.Script 
	local script = Instance.new('Script', Tab1)

	script.Parent.MouseButton1Down:Connect( function()
		script.Parent.Parent.Parent.Tab1.Visible = true
	end)
	
end
coroutine.wrap(ZPGV_fake_script)()
local function QFYNMAJ_fake_script() -- Tab2.Script 
	local script = Instance.new('Script', Tab2)

	script.Parent.MouseButton1Down:Connect( function()
		script.Parent.Parent.Parent.Tab1.Visible = true
		script.Parent.Parent.Parent.Tab2.Visible = false
	end)
end
coroutine.wrap(QFYNMAJ_fake_script)()
